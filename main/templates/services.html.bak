<!-- Update the JavaScript to launch modals -->
<script>
  document.addEventListener('DOMContentLoaded', function () {
    // Get all service items
    const serviceItems = document.querySelectorAll('.service-item');

    // Add click event listeners to each service item
    serviceItems.forEach((item, index) => {
      const readMoreBtn = item.querySelector('.readmore-btn');
      if (readMoreBtn) {
        readMoreBtn.addEventListener('click', function (e) {
          e.preventDefault();

          // Determine which modal to show based on the index or link
          const href = this.getAttribute('href');
          let modalId;

          if (href.includes('holistic-care')) {
            modalId = 'holisticCareModal';
          } else if (href.includes('education')) {
            modalId = 'educationModal';
          } else if (href.includes('healthcare')) {
            modalId = 'healthcareModal';
          } else if (href.includes('emotional-support')) {
            modalId = 'emotionalSupportModal';
          } else if (href.includes('skills')) {
            modalId = 'skillsModal';
          } else if (href.includes('community')) {
            modalId = 'communityModal';
          }

          if (modalId) {
            const modal = new bootstrap.Modal(document.getElementById(modalId));
            modal.show();
          }
        });
      }
    });
  });
</script>

